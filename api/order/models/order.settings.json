{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "name": "order"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "status": {
      "type": "enumeration",
      "enum": [
        "paid",
        "delivered",
        "unpaid"
      ]
    },
    "total": {
      "type": "decimal"
    },
    "checkout_session": {
      "type": "string"
    },
    "products": {
      "collection": "product"
    },
    "customer": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "orders"
    }
  }
}
